    "homepage": "https://github.com/jeroendesloovere/vcard",
    "license": "MIT",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Hans-JÃ¶rg Happel",
            "email": "happel@audriga.com",
            "homepage": "https://www.audriga.com",
            "role": "Developer"
        },
        {
            "name": "Jeroen Desloovere",
            "email": "info@jeroendesloovere.be",
            "homepage": "http://jeroendesloovere.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "behat/transliterator": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },
    "autoload": {
        "psr-4": { "JeroenDesloovere\\VCard\\": "src/" }
    },
    "archive": {
